# GoaGuide Backend Environment Variables

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/goaguide
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# Authentication
JWT_SECRET=your-super-secret-jwt-key-256-bits-long
CORS_ORIGIN=http://localhost:3000

# External APIs
OPENWEATHER_API_KEY={{OPENWEATHER_API_KEY}}
GOOGLE_MAPS_API_KEY={{GOOGLE_MAPS_API_KEY}}
OPENAI_API_KEY={{OPENAI_API_KEY}}

# File Storage
CLOUDINARY_URL=cloudinary://api_key:api_secret@cloud_name

# Application Settings
NODE_ENV=development
PORT=8080

# Redis (optional for caching)
REDIS_URL=redis://localhost:6379

# Feature Flags
FEATURE_EVENTS_INGEST=true
FEATURE_PROVIDER_RFP=true
FEATURE_AUTO_BOOK=false
FEATURE_PHOTO_VERIFICATION=true
FEATURE_LLM_ORCHESTRATOR=true
